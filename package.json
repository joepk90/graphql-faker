{
  "name": "graphql-faker-server",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "preversion": "npm ci --ignore-scripts && npm test",
    "test": "npm run lint && npm run check && npm run prettier:check && npm run check:spelling",
    "lint": "eslint --cache --max-warnings 0 .",
    "check": "tsc --noEmit",
    "start": "nodemon src/index.ts",
    "debug": "ts-node --inspect --compilerOptions '{\"inlineSources\":true}' src/index.ts",
    "build": "tsc",
    "serve:dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "serve:prod": "ts-node -r tsconfig-paths/register dist/index.js",
    "prettier": "prettier --cache --ignore-path .gitignore --write --list-different .",
    "prettier:check": "prettier --cache --ignore-path .gitignore --check .",
    "check:spelling": "cspell --cache --no-progress '**/*'"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "body-parser": "1.19.0",
    "chalk": "4.1.0",
    "cors": "2.8.5",
    "dotenv": "^16.5.0",
    "express": "4.17.1",
    "express-graphql": "0.12.0",
    "graphql": "14.7.0",
    "graphql-voyager": "1.0.0-rc.31",
    "moment": "2.29.1",
    "node-fetch": "^2.6.1",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.0",
    "@types/cors": "2.8.10",
    "@types/express": "^5.0.1",
    "@types/node": "20.2.5",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "cspell": "6.31.1",
    "eslint": "8.40.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "graphiql": "0.17.5",
    "marked": "1.1.0",
    "nodemon": "2.0.7",
    "prettier": "2.8.8",
    "ts-loader": "7.0.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
